    <script>
        // Animation d'apparition progressive des sections
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('revealed');
                }
            });
        }, observerOptions);

        // Observer toutes les sections
        document.querySelectorAll('.section-reveal').forEach(section => {
            observer.observe(section);
        });

        // Animation des barres de compétences
        const animateSkillBars = () => {
            const skillBars = document.querySelectorAll('.skill-bar');
            skillBars.forEach(bar => {
                const width = bar.classList.contains('w-full') ? '100%' :
                             bar.classList.contains('w-4/5') ? '80%' : '70%';
                bar.style.width = width;
            });
        };

        // Déclencher l'animation des compétences après un délai
        setTimeout(animateSkillBars, 1000);

        // Effet de parallaxe simple pour l'en-tête
        window.addEventListener('scroll', () => {
            const scrolled = window.pageYOffset;
            const header = document.querySelector('header');
            if (header) {
                header.style.transform = `translateY(${scrolled * 0.3}px)`;
            }
        });

        // Interaction hover pour les cartes technologiques
        document.querySelectorAll('.tech-card').forEach(card => {
            card.addEventListener('mouseenter', function() {
                this.style.transform = 'translateY(-5px) scale(1.02)';
            });
            
            card.addEventListener('mouseleave', function() {
                this.style.transform = 'translateY(0) scale(1)';
            });
        });

        // Smooth scroll pour les ancres
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Effect typing pour le titre terminal
        const terminalTitle = document.querySelector('.terminal-text');
        if (terminalTitle) {
            const originalText = terminalTitle.textContent;
            terminalTitle.textContent = '';
            
            let i = 0;
            const typeWriter = () => {
                if (i < originalText.length) {
                    terminalTitle.textContent += originalText.charAt(i);
                    i++;
                    setTimeout(typeWriter, 50);
                }
            };
            
            setTimeout(typeWriter, 1500);
        }
    </script>
